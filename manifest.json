{
  "manifest_version": 3,
  "name": "MaCalc",
  "description": "Calculate your Mastery Tracker averages",
  "version": "0.2",
  "action": {
    "default_popup": "popup/macalc.html",
    "default_icon": "macalc.png"
  },
"content_scripts": [
    {
      "matches": [
        "https://app.masteryconnect.com/student/classes/*/progress*"
      ],
      "js": ["grade_average.js"],
      "all_frames": true
    }
  ],
  "permissions": ["activeTab"]
}
